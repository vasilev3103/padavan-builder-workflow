############################################################
### ZyXEL Keenetic 4G III, версия B
############################################################

### Целевой производитель/продукт (поддерживается только Ralink RT3883/MT7620/MT7621/MT7628)
CONFIG_VENDOR=ZYXEL
CONFIG_PRODUCT=MT7628

### Целевой идентификатор продукта (board select, max 12 symbols)
CONFIG_FIRMWARE_PRODUCT_ID = «KN-4G3B»
############################################################
### Конфигурация ядра Linux
############################################################

### Включение режима сна процессора MT7628 (снижение тактовой частоты до 100 МГц в режиме ожидания) 
#CONFIG_FIRMWARE_CPU_SLEEP=y

### Выбор драйвера ядра для точки доступа Wi-Fi 2,4 ГГц
### 4.1 = MT7628 v4.1.0.0
CONFIG_FIRMWARE_WIFI2_DRIVER=4.1

### Включить поддержку IPv6
CONFIG_FIRMWARE_ENABLE_IPV6=y

### Включить поддержку USB
CONFIG_FIRMWARE_ENABLE_USB=y

### Включить поддержку файловой системы FAT/FAT32. ~0,1 МБ
#CONFIG_FIRMWARE_ENABLE_FAT=y

### Включить поддержку файловой системы exFAT (а также FAT/FAT32). ~0,12 МБ
CONFIG_FIRMWARE_ENABLE_EXFAT=y

### Включить поддержку файловой системы EXT2. ~0,1 МБ
#CONFIG_FIRMWARE_ENABLE_EXT2=y

### Включить поддержку файловой системы EXT3. ~0,2 МБ
#CONFIG_FIRMWARE_ENABLE_EXT3=y

### Включить поддержку файловой системы EXT4 (а также EXT3/2). ~0,4 МБ
CONFIG_FIRMWARE_ENABLE_EXT4=y

### Включить поддержку файловой системы XFS. ~0,6 МБ
#CONFIG_FIRMWARE_ENABLE_XFS=y

### Включить поддержку файловой системы Apple HFS+. ~0,1 МБ
#CONFIG_FIRMWARE_ENABLE_HFS=y

### Включить поддержку FUSE (файловых систем в пользовательском пространстве). ~0,1 МБ
CONFIG_FIRMWARE_ENABLE_FUSE=y

### Включить поддержку файлов подкачки/разделов. ~0,05 МБ
CONFIG_FIRMWARE_ENABLE_SWAP=y

### Включить модули UVC-камеры. ~0,2 МБ
#CONFIG_FIRMWARE_INCLUDE_UVC=y

### Включить модули USB-HID. ~0,2 МБ
#CONFIG_FIRMWARE_INCLUDE_HID=y

### Включите модули USB-Serial (например, pl2303). ~0,03 МБ
#CONFIG_FIRMWARE_INCLUDE_SERIAL=y

### Включите модули USB-Audio ~0,46 МБ
#CONFIG_FIRMWARE_INCLUDE_AUDIO=y

### Включите модули XFRM (IPsec) и расширение iptables ~ 0,2 МБ
#CONFIG_FIRMWARE_INCLUDE_XFRM=y

### Включите модули планирования сетевого QoS. ~0,2 МБ
#CONFIG_FIRMWARE_INCLUDE_QOS=y

### Включить модуль IMQ для формирователей (небольшое снижение производительности). ~0,02 МБ
#CONFIG_FIRMWARE_INCLUDE_IMQ=y

### Включить модуль IFB для формирователей. ~0,03 МБ
#CONFIG_FIRMWARE_INCLUDE_IFB=y

### Включить утилиту IPSet и модули ядра. ~0,4 МБ
CONFIG_FIRMWARE_INCLUDE_IPSET=y
### Включить сервер NFSv3. ~0,6 МБ
#CONFIG_FIRMWARE_INCLUDE_NFSD=y

### Включить клиент NFSv3. ~0,5 МБ
#CONFIG_FIRMWARE_INCLUDE_NFSC=y

### Включить клиент CIFS (SMB). ~0,2 МБ
CONFIG_FIRMWARE_INCLUDE_CIFS=y

### Включить сборку бэкпортированных драйверов для медиаустройств
#CONFIG_FIRMWARE_ENABLE_BACKPORTED=y
### Включить бэкпортированные драйверы в образ прошивки
### (в противном случае они будут собраны в отдельный архив для размещения в /opt/lib/)
#CONFIG_FIRMWARE_INCLUDE_BACKPORTED=y

############################################################
### Конфигурация пользовательского пространства
############################################################

